import java.util.Scanner;

public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        
        int h = sc.nextInt();
        int m = sc.nextInt();
        int t = sc.nextInt();
        m += t;
        
        if ( m >= 60 ) {
            h+=1;
            if ( h == 24 ) {
                h = 0;
            }
            m -= 60;
        }
        
        System.out.println(h + " " + m);
    }
}
